# exports
export GOOGLE_AUTH_PROXY_CLIENT_ID="id"
export GOOGLE_AUTH_PROXY_CLIENT_SECRET="sekrit"
export GOOGLE_AUTH_PROXY_COOKIE_SECRET="sekrit"
export GOOGLE_AUTH_PROXY_COOKIE_DOMAIN=".my-apps-domain.com"

# vars
google_auth_apps_domain="my-apps-domain.com"
google_auth_http_address="127.0.0.1:7001"
google_auth_redirect_url="https://internal.my-apps-domain.com/oauth2/callback"
google_auth_upstream="http://127.0.0.1:7002/"

GAP_DAEMON_ARGS="-google-apps-domain=${google_auth_apps_domain} -http-address=${google_auth_http_address} -redirect-url=${google_auth_redirect_url} -upstream=${google_auth_upstream}"
